{
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "10s",
  "httpHeaders": [
    {
      "urls": [
        "https://docs.github.com/",
        "https://help.github.com/"
      ],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^http(s)?://127.0.0.1.*"
    },
    {
      "pattern": "^http(s)?://localhost.*"
    }
  ]
}
